{% block title 'S inscrire' %}
{% block body %}
    <div class="container mt-4">
        <h2> Confirmer l'inscrpiton</h2>
        {% for message in app.flashes('warning') %}
            <div class="alert-danger">
                {{ message }}
            </div>
        {% endfor %}
        {% for message in app.flashes('message') %}
            <div class="alert-danger">
                {{ message }}
            </div>
        {% endfor %}
        {% for message in app.flashes('danger') %}
